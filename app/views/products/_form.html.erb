<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :overview %>
  <%= f.text_area :overview %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :unit %>
  <%= f.text_field :unit %>

  <%= f.label :user_id %>
  <%= f.text_field :user_id %>

  <%= f.submit %>
<% end %>
